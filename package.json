    "@storybook/react": "^3.4.10",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "4.8.0",
    "eslint-plugin-react": "7.4.0",
    "husky": "^0.14.3",
    "jest-styled-components": "^6.2.1",
